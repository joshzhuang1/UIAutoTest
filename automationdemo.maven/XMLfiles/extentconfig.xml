<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>

    <!-- report theme -->
    <!-- STANDARD, DARK -->
    <theme>STANDARD</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <!-- HTTP, HTTPS -->
    <protocol>HTTPS</protocol>

    <!-- offline report -->
    <timelineEnabled>true</timelineEnabled>
    
    <!-- offline report -->
    <enableOfflineMode>false</enableOfflineMode>

    <!-- title of the document -->
    <documentTitle>Automation Framework - JoshZ</documentTitle>

    <!-- report name - displayed at top-nav -->
    <reportName>
    
          <![CDATA[
			<img src='../logo4.jpg'/>
	      ]]>
    
    </reportName>

    <!-- timestamp format -->
    <timeStampFormat>dd MMM, yyyy HH:mm:ss</timeStampFormat>

    <!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
            
        });
      ]]>
    </scripts>

    <!-- custom styles -->
    <styles>
      <![CDATA[
      
      ]]>
    </styles>

  </configuration>
</extentreports>